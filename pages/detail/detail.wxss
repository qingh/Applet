page {
  color: white;
  background-repeat: no-repeat;
  background-image: url(data:image/jpg;base64,);
}
.wrap {
  padding: 0 20px;
  overflow: hidden;
  box-sizing: border-box;
}
.song {
  display: flex;
  margin-top: 50px;
}
.photo {
  position: relative;
}
.icon {
  width: 50px;
  height: 50px;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translateX(-50%) translateY(-50%);
  z-index: 10;
}
.img {
  width: 150px;
  height: 150px;
  border-radius: 50%;
  animation: rotate 15s infinite linear;
}
.msg {
  margin: 25px 0 0 25px;
}
.msg text {
  display: block;
  font-size: 16px;
}

.song-name {
  margin-top: 10px;
}
.album-name {
  margin-top: 10px;
}
/* .author {
  margin-top: 10px;
} */

.exception {
  text-align: center;
  margin-top: 200px;
}

@keyframes rotate {
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}
